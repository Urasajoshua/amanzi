{
    "version": 2,
    "regions": ["hnd1"],
    "builds": [
    {
        "src": "aru/wsgi.py",
        "use": "@vercel/python",
        "config": {
        "maxLambdaSize": "15mb"
        }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "static" }
    }
    ],
    "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/"
    },
    {
        "src": "/(.*)",
        "dest": "aru/wsgi.py"
    }
    ]
    }